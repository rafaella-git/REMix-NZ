11112          + sum(linksData$(links_aggregate(linksModelToCalc,linksData)
11113                          and transfer_incidenceData_flip(linksData) = -1
11114                          and transfer_linksParamIn(linksData,yearsToCalc,transfer_techs,"linksUpperLimit") = inf ),
