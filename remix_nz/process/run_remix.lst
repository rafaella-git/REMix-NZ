13233   
13234      + sum ((storage_techs(techs),vintage)
13235                  $storage_usedTech(nodesModelToCalc,yearsToCalc,storage_techs,vintage),
13236          storage_unitsDecom.l(nodesModelToCalc,yearsToCalc,storage_techs,vintage)
