6002      $(storage_usedTech(nodesModel,years,storage_techs,vintage)
6003        and storage_techParam(storage_techs,vintage,"mipUnits") = 1)
6004      = storage_reservoirParam(nodesModel,years,storage_techs,"unitsUpperLimit");
