6443          + sum(linksData$(links_aggregate(linksModelToCalc,linksData)
6444                          and transfer_incidenceData_flip(linksData) = -1
6445                          and transfer_linksParamIn(linksData,yearsToCalc,transfer_techs,"linksUpperLimit") = inf ),
