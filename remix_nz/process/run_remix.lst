10102      $(storage_usedTech(nodesModel,years,storage_techs,vintage)
10103        and storage_techParam(storage_techs,vintage,"mipUnits") = 1)
10104      = storage_reservoirParam(nodesModel,years,storage_techs,"unitsUpperLimit");
