1923              and not accounting_indicatorBounds(accNodesModel_a,accYears_a,indicator_a,"isVariable")
1924              and map_accNodes(accNodesModel_aa,accNodesModel_a)
1925              and map_accYears(accYears_aa,accYears_a))
