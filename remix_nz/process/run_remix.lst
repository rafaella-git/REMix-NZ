5738          + sum(linksData$(links_aggregate(linksModelToCalc,linksData)
5739                          and transfer_incidenceData_flip(linksData) = 1
5740                          and transfer_linksParamIn(linksData,years,transfer_techs,"linksUpperLimit") = inf),
